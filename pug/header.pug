include constants/nav.pug

header.header
    .container
        .header__content
            .inner
                a(href="#" class="saviour-care-logo")
                    div(class="saviour-care-logo__ellipse saviour-care-logo__ellipse--background-primary")
                        img(src="../assets/icons/logo/logo-shapes.png" alt="LOGO")
                    h5(class="saviour-care-logo__name saviour-care-logo__name--header-theme") saviour care
                nav.header__nav
                    ul.header__nav-list-items
                        each item in navItems
                            li
                                a(class="header__nav-item" href=item.url)=item.name